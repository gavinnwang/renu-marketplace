{
  "expo": {
    "backgroundColor": "#181818",
    "name": "Renu Marketplace",
    "slug": "marketplace-frontend",
    "version": "1.3.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "renu",
    "userInterfaceStyle": "automatic",
    "owner": "renu_marketplace",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "renu",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take picture of resell items for uploading."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F9F9F9"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a3301b86-dcea-4633-9984-26ee0412af35"
      }
    }
  }
}
