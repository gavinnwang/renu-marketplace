  "query": "\n        SELECT \n            ItemChat.id AS chat_id, \n            item_id,\n            buyer_id,\n            User.id AS seller_id, \n            Item.name AS item_name, \n            price, \n            category, \n            Item.created_at, \n            Item.updated_at, \n            description, \n            status\n        FROM ItemChat\n        JOIN Item ON ItemChat.item_id = Item.id\n        JOIN User ON Item.user_id = User.id\n        WHERE ItemChat.buyer_id = ?;\n        ",
