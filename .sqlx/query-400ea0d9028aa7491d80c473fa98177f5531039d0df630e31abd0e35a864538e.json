{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            item.id, \n            item.name, \n            item.price, \n            item.user_id,\n            item.description, \n            item.category::TEXT AS \"category!\",\n            item.status::TEXT AS \"status!\",\n            item.created_at,\n            item.updated_at,\n            item.images as images\n        FROM saved_item \n        INNER JOIN item ON saved_item.item_id = item.id\n        WHERE saved_item.user_id = $1\n        ORDER BY saved_item.created_at DESC\n        ",
