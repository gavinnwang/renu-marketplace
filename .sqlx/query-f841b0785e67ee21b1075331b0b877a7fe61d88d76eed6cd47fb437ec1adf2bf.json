  "query": "SELECT \n            u.id, \n            u.name, \n            u.email, \n            u.profile_image,\n            u.role::TEXT AS \"role!\",\n            u.created_at, \n            u.updated_at\n        FROM \"user\" u\n        LEFT JOIN \"item\" i ON u.id = i.user_id\n        WHERE u.id = $1\n        GROUP BY u.id;",
