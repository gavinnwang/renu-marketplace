  "db_name": "MySQL",
  "query": "\n        SELECT \n            Item.id, \n            Item.name, \n            Item.price, \n            Item.user_id,\n            Item.description, \n            Item.category,\n            Item.created_at,\n            Item.updated_at,\n            GROUP_CONCAT(ItemImage.url) AS item_images\n        FROM SavedItem\n        INNER JOIN Item ON SavedItem.itemId = Item.id\n        INNER JOIN ItemImage ON Item.id = ItemImage.item_id AND Item.id = SavedItem.itemId \n        WHERE SavedItem.userId = ?\n        GROUP BY Item.id\n        ",
