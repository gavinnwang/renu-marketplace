{
  "db_name": "MySQL",
  "query": "\n        SELECT\n            Message.id,\n            Message.chat_id,\n            Message.sender_id,\n            Message.content,\n            Message.created_at AS sent_at,\n            CASE\n                WHEN Message.sender_id = ? THEN 1\n                ELSE 0\n            END\n                AS from_me\n        FROM Message\n        WHERE Message.chat_id = ?\n        ORDER BY Message.created_at DESC\n        LIMIT 35;\n        ",
