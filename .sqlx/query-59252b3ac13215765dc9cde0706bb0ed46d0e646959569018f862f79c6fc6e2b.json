  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            item.id, \n            item.name, \n            item.price, \n            item.user_id, \n            item.category::TEXT AS \"category!\",\n            item.status::TEXT AS \"status!\",\n            item.created_at, \n            item.description,\n            item.updated_at,\n            item.images as images\n        FROM item\n        WHERE item.status = $1\n        ORDER BY item.created_at DESC\n        LIMIT $2\n        OFFSET $3;\n        ",
