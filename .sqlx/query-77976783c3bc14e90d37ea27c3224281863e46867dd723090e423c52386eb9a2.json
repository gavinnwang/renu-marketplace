  "query": "\n        SELECT\n            Item.id, \n            Item.name, \n            Item.price, \n            Item.user_id, \n            Item.category::TEXT AS \"category!\",\n            Item.status::TEXT AS \"status!\",\n            Item.description,\n            Item.created_at, \n            Item.updated_at,\n            Item.images as images\n        FROM Item\n        WHERE Item.user_id = $1 AND Item.status = $2\n        ORDER BY Item.created_at DESC\n        ",
